pipeline {
    agent any

    stages {
        stage('Build project') {
            steps {
                sh 'cd APCPPLib/'
                cmake installation: 'InSearchPath', arguments: '-B build .'
                cmakeBuild generator: 'Unix Makefiles', sourceDir: 'APCPPLib/', buildDir: 'build', buildType: 'Debug', installation: 'InSearchPath', steps: [[args: '..']]
            }
        }
    }
}